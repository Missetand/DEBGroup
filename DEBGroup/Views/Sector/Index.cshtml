@model DEBGroup.Models.Sector.Index

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DisplayName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.AllSectors) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SectorName)
        </td>
        <td>
            
            @Html.ActionLink("Edit", "Edit", new { id=item.SectorID }) |
            @Html.ActionLink("Details", "Index","Category", new { id = item.SectorID}, null) |
            @Html.ActionLink("Delete", "Delete", new { id=item.SectorID })

        </td>
    </tr>
}

</table>
